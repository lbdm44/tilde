##### BEGIN DEFAULT SETUP #####

[alias]
    alias = config --get-regexp alias
    br = branch
    co = checkout
    cb = checkout -b
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
    lgb = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit --branches
    ll = log --pretty=format:'%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]' --decorate --numstat
    st = status -sb
    unstage = reset --

[color]
    ui = auto

[color "status"]
    added = green
    changed = yellow
    untracked = cyan

[core]
    editor = code --wait

##### END DEFAULT SETUP #####
